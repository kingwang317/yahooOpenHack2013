{
  "name" : "Yahoo Open Hack Day 2013",
  "version" : "1.1",
  "description" : "Yahoo Open Hack Day 2013 Google Exts",
  "background" : {
    "scripts": ["background.js"]
  },
  "page_action" :
  {
    "default_icon" : "images/icon.png",
    "default_title" : "There's a 'sandwich' in this page!"
  },
  "icons": { 
           "48": "images/icon.png"},
  "content_scripts" : [
    {
      "matches" : [
       "http://www.reportgenie.net/yahoo_hack/*",
        "https://www.facebook.com/*"
      ],
      "js" : ["jquery.js","contentscript.js"],
      "css" : ["it4fun.css","reset.css"],
      "run_at" : "document_idle",
      "all_frames" : true
    }
  ],
   "web_accessible_resources": [
       "images/010d.png",
       "images/linkbtn.png",
       "images/listbtn.png",
       "images/rain.png",
       "images/sunrise.png",
       "images/sunset.png",
       "images/thermometer.png",
       "images/wind.png",
       "images/1.png",
       "images/2.png",
       "images/3.png",
       "images/4.png",
       "images/5.png"
     ],
  "manifest_version": 2
}